    <!-- JavaScript -->
    <script>
        document.getElementById('registerForm').addEventListener('submit', async function (e) {
            e.preventDefault(); // Prevent form from submitting the traditional way

            // Get the form data
            const username = document.getElementById('register-name').value;
            const email = document.getElementById('register-email').value;
            const password = document.getElementById('register-password').value;

            // Create an object to store form data
            const formData = {
                username: username,
                email: email,
                password: password
            };

            try {
                // Send a POST request to the registration API
                const response = await fetch('/user/register', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(formData)
                });

                const result = await response.json();

                // Check if the registration was successful
                if (response.status === 201) {
                    alert(result.msg); // Display success message
                    // Optionally, reset the form and close the modal
                    document.getElementById('registerForm').reset();
                    let registerModal = new bootstrap.Modal(document.getElementById('registerModal'));
                    registerModal.hide(); // Hide the modal
                } else {
                    // Display error message if something goes wrong
                    alert(result.msg || 'Error registering user');
                }
            } catch (error) {
                console.error('Error:', error);
                alert('There was an error with the registration. Please try again.');
            }
        });
    </script>

